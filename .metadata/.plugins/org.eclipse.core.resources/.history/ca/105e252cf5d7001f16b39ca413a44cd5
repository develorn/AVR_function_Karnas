#include <avr/io.h>
#include <avr/interrupt.h>

volatile uint8_t a;

volatile char wynikGSM[90]="abc";

void wyslij(char *data0)
{
int j;
int x=strlen(data0);
    for(j=0;j<x;j++)
    {
    usart_transmit(data0[j]);
    }
}


int main(void) {

	uint8_t b=0, c=1;

	while(1) {
		b=c+a;
		c++;
		if( b>16 ) {
			b=0;
			PORTB = b;
			a=2;
			PORTA = a;
		}
	}

}


ISR(INT0_vect) {
	a++;
}
