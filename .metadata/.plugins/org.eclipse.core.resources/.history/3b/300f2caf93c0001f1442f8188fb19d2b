//*************************************************
unsigned char receiveByte( void )
{
	unsigned char data, status;
	
	while(!(UCSRA & (1<<RXC))); 	// Wait for incomming data
    2878:	5f 9b       	sbis	0x0b, 7	; 11
    287a:	fe cf       	rjmp	.-4      	; 0x2878 <receiveByte>
	
