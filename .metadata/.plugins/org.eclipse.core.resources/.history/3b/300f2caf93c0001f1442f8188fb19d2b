000030c8 <i2c_start>:
//Function to start i2c communication
//*************************************************
unsigned char i2c_start(void)
{
 
	TWCR = (1<<TWINT)|(1<<TWSTA)|(1<<TWEN); 	//Send START condition
    30c8:	84 ea       	ldi	r24, 0xA4	; 164
    30ca:	86 bf       	out	0x36, r24	; 54
	
    while (!(TWCR & (1<<TWINT)));   		//Wait for TWINT flag set. This indicates that the
    30cc:	06 b6       	in	r0, 0x36	; 54
    30ce:	07 fe       	sbrs	r0, 7
    30d0:	fd cf       	rjmp	.-6      	; 0x30cc <i2c_start+0x4>
		  								//START condition has been transmitted
    if ((TWSR & 0xF8) == START)			//Check value of TWI Status Register
    30d2:	81 b1       	in	r24, 0x01	; 1
    30d4:	90 e0       	ldi	r25, 0x00	; 0
    30d6:	88 7f       	andi	r24, 0xF8	; 248
    30d8:	88 30       	cpi	r24, 0x08	; 8
    30da:	09 f0       	breq	.+2      	; 0x30de <i2c_start+0x16>
    30dc:	91 e0       	ldi	r25, 0x01	; 1
 	   return(0);
	else
	   return(1);
}
    30de:	89 2f       	mov	r24, r25
    30e0:	08 95       	ret

000030e2 <i2c_repeatStart>:
//Function for repeat start condition
//*************************************************
unsigned char i2c_repeatStart(void)
{
 
    TWCR = (1<<TWINT)|(1<<TWSTA)|(1<<TWEN); 		//Send START condition
    30e2:	84 ea       	ldi	r24, 0xA4	; 164
    30e4:	86 bf       	out	0x36, r24	; 54
    while (!(TWCR & (1<<TWINT)));   		//Wait for TWINT flag set. This indicates that the
    30e6:	06 b6       	in	r0, 0x36	; 54
    30e8:	07 fe       	sbrs	r0, 7
    30ea:	fd cf       	rjmp	.-6      	; 0x30e6 <i2c_repeatStart+0x4>
		  								//START condition has been transmitted
    if ((TWSR & 0xF8) == REPEAT_START)			//Check value of TWI Status Register
    30ec:	81 b1       	in	r24, 0x01	; 1
    30ee:	90 e0       	ldi	r25, 0x00	; 0
    30f0:	88 7f       	andi	r24, 0xF8	; 248
