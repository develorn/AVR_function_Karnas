    286c:	89 e1       	ldi	r24, 0x19	; 25
    286e:	89 b9       	out	0x09, r24	; 9
 UBRRH = 0x00; //set baud rate hi
    2870:	10 bc       	out	0x20, r1	; 32
 UCSRB = 0x18;
    2872:	88 e1       	ldi	r24, 0x18	; 24
    2874:	8a b9       	out	0x0a, r24	; 10
}
