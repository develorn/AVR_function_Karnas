00002846 <spi_init>:

//SPI initialize for SD card
//clock rate: 125Khz
void spi_init(void)
{
SPCR = 0x52; //setup SPI: Master mode, MSB first, SCK phase low, SCK idle low
    2846:	82 e5       	ldi	r24, 0x52	; 82
    2848:	8d b9       	out	0x0d, r24	; 13
SPSR = 0x00;
    284a:	1e b8       	out	0x0e, r1	; 14
}
    284c:	08 95       	ret

0000284e <SPI_transmit>:

unsigned char SPI_transmit(unsigned char data)
{
// Start transmission
SPDR = data;
    284e:	8f b9       	out	0x0f, r24	; 15

// Wait for transmission complete
while(!(SPSR & (1<<SPIF)));
    2850:	77 9b       	sbis	0x0e, 7	; 14
    2852:	fe cf       	rjmp	.-4      	; 0x2850 <SPI_transmit+0x2>
data = SPDR;
    2854:	8f b1       	in	r24, 0x0f	; 15

return(data);
}
