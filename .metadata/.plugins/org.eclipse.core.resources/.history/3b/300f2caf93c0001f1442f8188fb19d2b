0000317a <i2c_stop>:
//**************************************************
//Function to end the i2c communication
//*************************************************   	
void i2c_stop(void)
{
  TWCR =  (1<<TWINT)|(1<<TWEN)|(1<<TWSTO);	  //Transmit STOP condition
    317a:	84 e9       	ldi	r24, 0x94	; 148
    317c:	86 bf       	out	0x36, r24	; 54
}  
    317e:	08 95       	ret

